<?xml version="1.0" encoding="UTF-8"?>
<model name="units_test" xmlns="http://www.cellml.org/cellml/1.1#" xmlns:cellml="http://www.cellml.org/cellml/1.1#" xmlns:cmeta="http://www.cellml.org/metadata/1.0#" cmeta:id="units_test_model_id" xmlns:xlink="http://www.w3.org/1999/xlink">
  <import xlink:href="units-imported.xml">
    <units name="pms_imported" units_ref="pms"/>
  </import>
  <component name="component" cmeta:id="component_id">
    <variable name="alpha" units="pms_imported"/>
    <math xmlns="http://www.w3.org/1998/Math/MathML" cmeta:id="units_test_eq">
      <apply id="units_test_calculation"><eq/>
        <ci>alpha</ci>
        <cn cellml:units="pms_imported">0.1</cn>
      </apply>
    </math>
  </component>
</model>
